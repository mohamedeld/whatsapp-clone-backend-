{
  "version": 2,
  "routes": [
    { "src": "/configs/(.*)", "dest": "/src/configs/$1" },
    { "src": "/controller/(.*)", "dest": "/src/controller/$1" },
    { "src": "/middleware/(.*)", "dest": "/src/middleware/$1" },
    { "src": "/model/(.*)", "dest": "/src/model/$1" },
    { "src": "/routes/(.*)", "dest": "/src/routes/$1" },
    { "src": "/services/(.*)", "dest": "/src/services/$1" },
    { "src": "/utils/(.*)", "dest": "/src/utils/$1" },
    { "src": "/(.*)", "dest": "/src/$1" }
  ]
}